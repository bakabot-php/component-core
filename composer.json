{
    "name": "bakabot/core",
    "description": "Provides the core functionality and facilities of the bot.",
    "license": "MIT",
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.18",
        "phpunit/phpunit": "^9.3",
        "roave/security-advisories": "dev-master",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "files": [
            "src/functions.php"
        ],
        "psr-4": {
            "Bakabot\\Attribute\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bakabot\\Attribute\\": "tests/"
        }
    },
    "config": {
        "lock": false,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
